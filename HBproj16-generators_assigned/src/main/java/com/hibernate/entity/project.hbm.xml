<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.entity.project">
	<id name="proid">
	<generator class="assigned"></generator>
	</id>
	<property name="projName" column="projName" type="string" length="20"></property>
	<property name="teamSize" column="teamSize" type="int" length="20"></property>
	<property name="company" column="company" type="string" length="20"></property>
	</class>
</hibernate-mapping>