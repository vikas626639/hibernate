<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.entity.BankAccount">
		<id name="accno">
			<generator class="identity">
			</generator>
		</id>
		<timestamp name="lastUpdate"></timestamp>
		<property name="holderName" type="string"></property>
		<property name="balance" type="double"></property>
		<property name="type" type="string"></property>
		<property name="openingDate" ></property>
	</class>
</hibernate-mapping>