<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.entity.Programmers_project_work">
		<composite-id name="cid"
			class="com.hibernate.composite.Composite">
			<key-property name="pid" column="pid" type="int" />
			<key-property name="projid" column="projid" type="int" />
		</composite-id>
		<property name="pname" column="pname" type="string"></property>
		<property name="projName" column="projName" type="string"></property>
		<property name="salary" column="salary" type="double"></property>
	</class>
</hibernate-mapping>