<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hibernate.entity.MobileCustomer">
		<id name="customer_id">
			<generator class="identity">
			</generator>
		</id>
		<version name="versionCount" column="Modify" />
		<property name="customer_name" type="string" />
		<property name="MobileNO" type="long"/>
		<property name="callerTune" type="string" />
	</class>
</hibernate-mapping>